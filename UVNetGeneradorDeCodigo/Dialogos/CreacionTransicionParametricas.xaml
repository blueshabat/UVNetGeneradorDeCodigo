<API:CDialogo  x:Class="UVNetGeneradorDeCodigo.Dialogos.CreacionTransicionParametricas"
               x:TypeArguments="ScriptsSQL:CTransicionParametricas"
               xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
               xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
               xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
               xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
               xmlns:API="clr-namespace:UVNetGeneradorDeCodigo.API"
               xmlns:ScriptsSQL="clr-namespace:UVNetGeneradorDeCodigo.Generador.ScriptsSQL"
               mc:Ignorable="d" WindowStartupLocation="CenterScreen"
             d:DesignHeight="450" d:DesignWidth="300">
    <Grid>
        <StackPanel Orientation="Vertical">
            <Label Content="Paramétrica" Margin="10,0,0,0"/>
            <TextBox x:Name="TxtProcesoDescripcion" Height="23" TextWrapping="Wrap" Text="" MaxLength="40" MaxLines="1" CharacterCasing="Upper" Margin="10,0" TextChanged="TxtProcesoDescripcion_TextChanged"/>
            <Label Content="Nombre corto" Margin="10,0,0,0"/>
            <TextBox x:Name="TxtProceso" Height="23" TextWrapping="Wrap" Text="" MaxLength="5" MaxLines="1" CharacterCasing="Upper" Margin="10,0"/>
            <Button Content="Crear" Click="BtnCrear_Click" Height="22" Margin="0,15" HorizontalAlignment="Center" MinWidth="50"/>
        </StackPanel>
    </Grid>
</API:CDialogo>
